<p><div class="notice"><%= flash[:notice] %></div></p>
<main>
  <div class="container px-5 px-sm-0">
    <div class="row">
      <div class="col-md-3">
        <%= render 'books/list', book: @book, user: @user %>
      </div>
      <div class="col-md-8 offset-md-1">
        <h1>Users</h1>
          <table class="table table-hover table-inverse">
            <thead>
             <tr>
             <th><p>image</p></th>
             <th><p>name</p></th>
             </tr>
          </thead>
       <% @users.each do |user| %>
             <tr>
             <td><p><%= image_tag user.get_profile_image(100,100) %></p></td>
             <td><p><%= user.name %></p></td>
             <td><p><%= link_to "Show", user_path(user.id) %></p></td>
             </tr>
       <% end %>
         </table>
      </div>
    </div>
  </div>
</main








